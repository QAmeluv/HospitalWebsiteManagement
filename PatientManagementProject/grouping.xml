<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="GroupSuite">
	<listeners>
		<listener class-name="extentReport.TestListener"></listener>
	</listeners>
	<test thread-count="5" name="Test">
		<parameter name="browser" value="chrome"></parameter>
		<groups>

			<run>
				<include name="launch"></include>
				<include name="basicfunctionality"></include>
				<include name="patientEdits"></include>
				<include name="serviceRelated"></include>
				<include name="close"></include>
			</run>
		</groups>
		<classes>
			<class name="testClasses.LoginPageTestClass" />
			<class name="testClasses.HomePageTestClass"/>
			<class name="testClasses.FindPatientTestClass" />
			<class name="testClasses.RegisterAPatientTestClass" />
			<class name="testClasses.AppointmentSchedulingTestClass" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->


